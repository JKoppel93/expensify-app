{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jest/all"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "jest"],
  "rules": {},
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}
